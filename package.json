{
  "name": "@teritorio/openstreetmap-logical-history-component",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "yarn@4.9.2",
  "description": "An OpenStreetMap logical history (LoCha) UI component.",
  "author": "Teritorio",
  "license": "MIT",
  "homepage": "https://github.com/teritorio/openstreetmap-logical-history-component#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teritorio/openstreetmap-logical-history-component.git"
  },
  "bugs": {
    "url": "https://github.com/teritorio/openstreetmap-logical-history-component/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:demo": "vite build --mode demo --emptyOutDir",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preversion": "yarn build",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "test": "vitest"
  },
  "peerDependencies": {
    "maplibre-gl": "^4.7.1",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@turf/area": "^7.2.0",
    "@turf/bbox": "^7.2.0",
    "@turf/boolean-equal": "^7.2.0",
    "@vueuse/components": "^13.7.0",
    "@vueuse/core": "^13.7.0",
    "diff": "^8.0.2",
    "underscore": "^1.13.7",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/underscore": "^1.13.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.33.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^16.1.5",
    "maplibre-gl": "^5.6.2",
    "simple-git-hooks": "^2.13.1",
    "typescript": "~5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.5"
  },
  "resolutions": {
    "@microsoft/api-extractor/typescript": "^5.6.2"
  },
  "simple-git-hooks": {
    "commit-msg": "yarn dlx commitlint --edit $1",
    "pre-commit": "yarn dlx lint-staged",
    "preserveUnused": true
  },
  "lint-staged": {
    "*": "yarn lint:fix"
  }
}
